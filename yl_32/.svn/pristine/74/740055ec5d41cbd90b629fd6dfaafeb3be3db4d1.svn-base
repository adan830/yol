[Paysys]
#Paysys的地址和开放端口，注意，如果Bishop机器和Paysys机器都有块外网网卡的话，就填Paysys的外网IP！
IP=10.20.86.105
Port=8641
#连接Paysys的用户名和密码,在剑网旧Paysys中,这个是在Paysys的数据库里面定义的,在KOL新的Paysys中,这个在配置文件ggw.conf中
UserName=zhaoyu1
Password=waiwan22
#多少秒没收到paysys回包认为是断开
PingTimeout=120
PingCycle=10
#内网地址掩码，程序内部用它来标示“形如什么样的地址是内网地址”
InternalIPMask=10.20.0.0
# 本地IP配置暂时无效
LocalIP=
#LocalMAC=01-01-01-01-01-02

[Relay]
IP=localhost
Port=7701
PingCycle=10

[Player]
MaxPlayer=10000
IP=
Port=5622
PingCycle=100

[Global]
Locale=
# 账户服务器：none、paysys、sdoa(盛大)或ldap（AD域验证）
AccountServer=none
# 是否启用白名单限制(whitelist.txt)
UseWhiteList=no

[LDAP]
# AD域控服务器地址
URI=dev.qiwan.net
# 域名
Domain=rdev
# 加密方法simple、ntlm、digest-md5等
SASL=DIGEST-MD5

